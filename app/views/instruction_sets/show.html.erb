<p id="notice"><%= notice %></p>

<p>
  <b>Name:</b>
  <%= @set_name%>
</p>

<table>
<% @current_set_list.each do |instruction| %>
  <tr>
    <td><%= instruction.name %></td>
    <td><%= link_to 'Show', instruction %></td>
    <td><%= link_to 'Edit', edit_instruction_path(instruction) %></td>
    <td><%= link_to 'Destroy', instruction, method: :delete, data: { confirm: 'Are you sure?' } %></td>
  </tr>
<% end %>
</table>

<br />

<%= link_to 'Execute Instruction Set', @instruction_set, method: :execute %>

<%= link_to 'New Instruction', new_instruction_path(:set_id => params[:id])%>

<%= link_to 'Edit', edit_instruction_set_path(@instruction_set) %> |
<%= link_to 'Back', instruction_sets_path %>
